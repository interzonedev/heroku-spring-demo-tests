<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<import resource="classpath:com/interzonedev/herokuspringdemo/spring/applicationContext.xml" />

	<bean id="dbUrl" class="java.net.URI">
		<constructor-arg
			value="postgres://herokuspringdemouser:herokuspringdemopass@localhost/herokuspringdemo_test" />
	</bean>

	<bean id="dbUnitDataSetTester" class="com.interzonedev.sprintfix.dataset.dbunit.DbUnitDataSetTester" />
</beans>
